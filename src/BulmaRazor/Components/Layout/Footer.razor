@namespace BulmaRazor.Components
@inherits BulmaComponentBase

<footer @attributes="Attributes" class="@classes">
    @ChildContent
</footer>


@code {

    string classes => CssBuilder.Default("footer")
        .AddClassFromAttributes(Attributes)
        .Build();

    /// <summary>
    /// 子内容
    /// </summary>
    [Parameter]
    public RenderFragment ChildContent { get; set; }

}